import org.w3c.dom.Node;

public class Node {
	int data;
	Node next;
	public Node(int data) {
		this.data = data;
	}
	
	public Node next() {
		return this.next;
	}
	
	public static int countNodes(Node head) {
		int total=1;
		Node data = head.next();
		while(data.next != null) {
			total += 1;
			data = data.next;
		}
		return total;
	}
	public static void main(String[] args) {
		Node head = 
	}

}
